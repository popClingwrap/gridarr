{
  "name": "gridarr",
  "version": "1.1.0",
  "type": "module",
  "description": "Represents an Array of data as a grid and allows grid-like referencing and manipulation",
  "main": "dist/gridarr.js",
  "module": "./dist/gridarr.js",
  "types": "./dist/gridarr.d.ts",
  "repository": "https://github.com/popClingwrap/gridarr",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"tsc -w -p .\" \"live-server --open=./dev/index.html\"",
    "ts": "tsc -w -p .",
    "js": "node --watch index.js",
    "build": "tsup src/gridarr.ts --dts --out-dir dist"
  },
  "keywords": [
    "grid",
    "data",
    "list"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2",
    "live-server": "^1.2.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
